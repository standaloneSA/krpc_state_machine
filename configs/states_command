[init]

[pre_hold]
set_launch_profile = leo_normal
set_launch_profile = leo_steep
# seconds till launch
enable_sas = True
launch_time = 11
time_in_state = 8
following_state = startup
enable_rcs = False
command_throttle = 0
set_target_apoapsis = 120000
set_target_periapsis = 120000

[startup]
time_in_state = 1
following_state = ignition
command_throttle = 0

[ignition]
time_in_state = 2
following_state = launch
command_stage = True
command_throttle = 20

[launch]
enable_sas = True
command_throttle = 100
command_pitch = 90
command_heading = 90
set_target_altitude = 0.1
following_state = begin_turn
command_stage = True

[begin_turn]
enable_sas = True
enable_rcs = False
command_throttle = 75
command_pitch = 87.5
set_target_altitude = 1
following_state = ascent_1

[ascent_1]
enable_sas = True
command_throttle = 75
command_heading = 90
command_pitch = keep
set_target_speed = mach1
following_state = transsonic
set_target_altitude = 

[transsonic]
enable_sas = True
set_target_speed = None
command_throttle = 60
set_target_altitude = 9
following_state = ascent_2
command_pitch = keep

[ascent_2]
mach_check = False
enable_sas = True
command_throttle = 70
command_pitch = follow_path
enable_rcs = True
set_target_altitude = 60
following_state = coast

[coast]
enable_sas = True
enable_rcs = True
command_throttle = 0
command_pitch = 0
following_state = circularize
set_target_altitude = 119

[outta_gas]
command_throttle = 0

[shutdown_abort]
enable_rcs = False
command_throttle = 0
self_destruct = True

[circularize]
command_throttle = 80
command_pitch = 0
set_target_altitude = 99999
following_state = safe

[abort]
command_throttle = 0
enable_rcs = False
self_destruct = True
command_stage = False

[post_abort]
command_throttle = 0
enable_sas = False
enable_rcs = False
command_stage = False

[safe]
command_throttle = 0
enable_sas = False
enable_rcs = False
command_stage = False

