[safe]
command_throttle = 0
enable_sas = False
enable_rcs = False
command_stage = False

[reentry]
command_throttle = 0
check_for_chute_deploy = True
command_pitch = retrograde
command_stage = True
following_state = deploy_chutes

[deploy_chutes]
deploy_chutes = True
following_state = welcome_home

[welcome_home]
following_state = safe

[safe_orbit]
following_state = deorbit_burn
