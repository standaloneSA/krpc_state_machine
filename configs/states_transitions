[pre_hold]
initial_state = True
transitions = startup,safe
abort = safe

[startup]
transitions = engine_verify,shutdown_abort
abort = shutdown_abort

[engine_verify]
transitions = go_launch,shutdown_abort
abort = shutdown_abort

[go_launch]
transitions = ascent,shutdown_abort
abort = shutdown_abort

[ascent_1]
transitions = abort,outta_gas,coast
abort = abort

[ascent_2]
transitions = abort,outta_gas,coast
abort = abort

[outta_gas]
transitions = ballistic,abort
abort = abort

[shutdown_abort]
transitions = safe
abort = safe

[coast]
transitions = abort,ascent_2
abort = abort

[ballistic]
transitions = abort
abort = abort

[abort]
transitions = post_abort
abort = post_abort

[post_abort]
transitions =
abort = post_abort

[safe]
transitions = 
abort = safe
